<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trifecta.team.qrich">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/qrichsquareblue"
        android:label="@string/app_name"
        android:roundIcon="@drawable/qrichsquareblue"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="barcode" />

        <activity android:name=".CuisineActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BarcodeAvtivity" />
        <activity android:name=".SmsCodeActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".RestaurantActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".ItemActivity" />
        <activity android:name=".CartActivity"></activity>
    </application>

</manifest>